{
  "name": "t3-cloudflare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "next build",
    "db:up": "pnpm with-env drizzle-kit up",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:push": "pnpm with-env drizzle-kit push",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:check": "pnpm with-env drizzle-kit check",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "dev": "pnpm with-env next dev",
    "lint": "pnpm with-env next lint",
    "pages:dev": "pnpm wrangler pages dev",
    "pages:build": "pnpm with-env next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy",
    "with-env": "dotenv -e .dev.vars --"
  },
  "dependencies": {
    "@libsql/client": "^0.6.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.39.0",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "cf-bindings-proxy": "^0.7.1",
    "drizzle-orm": "^0.30.10",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20240529.0",
    "@types/eslint": "^8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "better-sqlite3": "^9.4.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-next-on-pages": "^1.11.3",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2",
    "vercel": "^34.2.4",
    "wrangler": "^3.58.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  },
  "packageManager": "pnpm@9.1.1"
}
