{
  "name": "t3-cloudflare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "next build",
    "db:up": "pnpm with-env drizzle-kit up",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:push": "pnpm with-env drizzle-kit push",
    "db:push:local": "pnpm with-env pnpm with-local-db drizzle-kit push --config drizzle.local.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:migrate:local": "pnpm with-env pnpm with-local-db drizzle-kit migrate --config drizzle.local.config.ts",
    "db:check": "pnpm with-env drizzle-kit check",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "db:studio:local": "pnpm with-env pnpm with-local-db drizzle-kit studio --config drizzle.local.config.ts",
    "d1:migrate:local": "wrangler d1 migrations apply prod-d1 --local",
    "d1:migrate:remote": "wrangler d1 migrations apply prod-d1 --remote",
    "dev": "pnpm with-env next dev",
    "lint": "pnpm with-env next lint",
    "pages:dev": "pnpm wrangler pages dev",
    "pages:build": "pnpm with-env next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy",
    "with-env": "dotenv -e .dev.vars --",
    "with-local-db": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.39.0",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "cf-bindings-proxy": "^0.7.1",
    "drizzle-orm": "^0.30.10",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.12.0",
    "@cloudflare/workers-types": "^4.20240529.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/eslint": "^8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-next-on-pages": "^1.12.0",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2",
    "vercel": "^34.2.4",
    "wrangler": "^3.58.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  },
  "packageManager": "pnpm@9.1.1"
}

