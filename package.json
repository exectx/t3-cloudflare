{
  "name": "t3-cloudflare",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:up": "drizzle-kit up:sqlite",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate:local": "wrangler d1 migrations apply t3-cloudflare --local",
    "db:migrate:prod": "wrangler d1 migrations apply t3-cloudflare",
    "db:migrate:prod:remote": "wrangler d1 migrations apply t3-cloudflare --remote",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview t3-cloudflare-preview",
    "db:studio:local": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env DB_NAME='t3-cloudflare-preview' WRANGLER_CONFIG='--environment=preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:dev": "pnpm wrangler pages dev",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.39.0",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "cf-bindings-proxy": "^0.7.1",
    "drizzle-orm": "^0.30.10",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20240529.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/eslint": "^8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "better-sqlite3": "^9.4.3",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-next-on-pages": "^1.11.3",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2",
    "wrangler": "^3.58.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  },
  "packageManager": "pnpm@9.1.1"
}
